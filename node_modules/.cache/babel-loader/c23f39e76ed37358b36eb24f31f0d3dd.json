A;AACA,QAAA,KAAA,EAAA,UADA;AAEA,QAAA,IAAA,EAAA,iBAFA;AAGA,QAAA,EAAA,EAAA;AAHA,OAnBA,EAyBA;AACA,QAAA,KAAA,EAAA,mBADA;AAEA,QAAA,IAAA,EAAA,wBAFA;AAGA,QAAA,EAAA,EAAA;AAHA,OAzBA;AADA,KAAA;AAAA,GARA;AAyCA,EAAA,QAAA,kCAEA,QAAA,CAAA;AACA,IAAA,YAAA,EAAA,sBAAA,KAAA;AAAA,aAAA,KAAA,CAAA,aAAA,CAAA,YAAA;AAAA,KADA;AAEA,IAAA,SAAA,EAAA,mBAAA,KAAA;AAAA,aAAA,KAAA,CAAA,aAAA,CAAA,SAAA;AAAA;AAFA,GAAA,CAFA;AAOA,IAAA,cAAA,EAAA;AACA,MAAA,GADA,iBACA;AACA,eAAA,KAAA,MAAA,CAAA,KAAA,CAAA,aAAA,CAAA,cAAA;AACA,OAHA;AAIA,MAAA,GAJA,eAIA,GAJA,EAIA;AACA,aAAA,MAAA,CAAA,MAAA,CAAA,oBAAA,EAAA,GAAA;AACA;AANA;AAPA,IAzCA;AAyDA,EAAA,KAAA,EAAA;AACA,mCADA,uCACA,GADA,EACA;AACA,WAAA,KAAA,CAAA,sBAAA,EAAA,CAAA,GAAA;AACA;AAHA,GAzDA;AA+DA,EAAA,OAAA,EAAA;AA/DA,CAAA","sourcesContent":["<template>\n  <v-navigation-drawer\n    v-model=\"Sidebar_drawer\"\n    :dark=\"SidebarColor !== 'white'\"\n    :color=\"SidebarColor\"\n    mobile-breakpoint=\"960\"\n    clipped\n    :right=\"$vuetify.rtl\"\n    mini-variant-width=\"70\"\n    :expand-on-hover=\"expandOnHover\"\n    app\n    id=\"main-sidebar\"\n  >\n    <v-list dense nav>\n      <!---USer Area -->\n      <!-- <v-list-item two-line class=\"px-0\">\n        <v-list-item-avatar>\n          <img src=\"https://randomuser.me/api/portraits/men/81.jpg\" />\n        </v-list-item-avatar>\n\n        <v-list-item-content>\n          <v-list-item-title>Dohn Deo</v-list-item-title>\n          <v-list-item-subtitle class=\"caption\"\n            >Webdesigner</v-list-item-subtitle\n          >\n        </v-list-item-content>\n      </v-list-item> -->\n      <!---USer Area -->\n      <!---Sidebar Items -->\n      <v-list-item\n        v-for=\"item in items\"\n        :key=\"item.title\"\n        :to=\"item.to\"\n        :active-class=\"`success white--text`\"\n        link\n      >\n        <v-list-item-icon>\n          <v-icon>{{ item.icon }}</v-icon>\n        </v-list-item-icon>\n\n        <v-list-item-content>\n          <v-list-item-title>{{ item.title }}</v-list-item-title>\n        </v-list-item-content>\n      </v-list-item>\n      <!---Sidebar Items -->\n    </v-list>\n  </v-navigation-drawer>\n</template>\n\n<script>\nimport { mapState } from \"vuex\";\n\nexport default {\n  name: \"Sidebar\",\n  props: {\n    expandOnHover: {\n      type: Boolean,\n      default: false,\n    },\n  },\n  data: () => ({\n    items: [\n      {\n        title: \"Dashboard\",\n        icon: \"mdi-view-dashboard\",\n        to: \"/admin\",\n      },\n\n      {\n        title: \"Users\",\n        icon: \"mdi-account-cog\",\n        to: \"/profile\",\n      },\n\n      {\n        title: \"Events\",\n        icon: \"mdi-application\",\n        to: \"/events\",\n      },\n\n      {\n        title: \"Merchant\",\n        icon: \"mdi-application\",\n        to: \"/merchant\",\n      },\n\n      {\n        title: \"Historical Report\",\n        icon: \"mdi-table-column-width\",\n        to: \"/tables-simple\",\n      },\n    ],\n  }),\n  computed: {\n    // ...mapState([\"SidebarColor\", \"SidebarBg\"]),\n    ...mapState({\n      SidebarColor: (state) => state.SidebarModule.SidebarColor,\n      SidebarBg: (state) => state.SidebarModule.SidebarBg,\n    }),\n\n    Sidebar_drawer: {\n      get() {\n        return this.$store.state.SidebarModule.Sidebar_drawer;\n      },\n      set(val) {\n        this.$store.commit(\"SET_SIDEBAR_DRAWER\", val);\n      },\n    },\n  },\n  watch: {\n    \"$vuetify.breakpoint.smAndDown\"(val) {\n      this.$emit(\"update:expandOnHover\", !val);\n    },\n  },\n\n  methods: {},\n};\n</script>\n<style lang=\"scss\">\n#main-sidebar {\n  box-shadow: 1px 0 20px rgba(0, 0, 0, 0.08);\n  -webkit-box-shadow: 1px 0 20px rgba(0, 0, 0, 0.08);\n  .v-navigation-drawer__border {\n    display: none;\n  }\n  .v-list {\n    padding: 8px 15px;\n  }\n  .v-list-item {\n    &__icon--text,\n    &__icon:first-child {\n      justify-content: center;\n      text-align: center;\n      width: 20px;\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/layouts/sidebar"}]}