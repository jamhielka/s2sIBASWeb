AwBA;AACA;AACA;AACA;AACA;AACA;;AA7BA;AAAA,uBA+BA,KAAA,CAAA,IAAA,CACA,IADA,CACA,QADA,EACA,QADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,QAAA,EADA,CAEA;AACA;;AACA,kBAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA,KAAA;;AACA,kBAAA,KAAA,CAAA,MAAA,GAAA,KAAA;AACA,kBAAA,KAAA,CAAA,gCAAA,CAAA;AACA,iBATA,EAUA,KAVA,CAUA,UAAA,CAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,kBAAA,KAAA,CAAA,UAAA,GAAA,KAAA;AACA,iBAbA,CA/BA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA+CA,KAzDA;AA2DA,IAAA,WA3DA,yBA2DA;AACA,WAAA,MAAA,GAAA,KAAA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,KAAA;AACA,KA9DA;AAgEA,IAAA,UAhEA,sBAgEA,CAhEA,EAgEA;AACA,aAAA,MAAA,CAAA,CAAA,CAAA,CAAA,MAAA,CAAA,qBAAA,CAAA;AACA;AAlEA,GA/CA;AAmHA,EAAA,QAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,aAAA,KAAA,MAAA,CAAA,OAAA,CAAA,UAAA;AACA;AAHA;AAnHA,CAAA","sourcesContent":["<template>\r\n  <v-row>\r\n    <v-col cols=\"12\">\r\n      <v-dialog v-model=\"dialog\" persistent max-width=\"600px\">\r\n        <template v-slot:activator=\"{ on, attrs }\">\r\n          <v-btn color=\"primary\" v-bind=\"attrs\" v-on=\"on\">\r\n            <v-icon class=\"mr-2\">mdi-plus-circle</v-icon> Add Events\r\n          </v-btn>\r\n        </template>\r\n        <v-card>\r\n          <v-form ref=\"form\" lazy-validation>\r\n            <v-card-title>\r\n              <span>Add Events</span>\r\n            </v-card-title>\r\n            <v-card-text>\r\n              <v-container>\r\n                <v-row>\r\n                  <v-col cols=\"12\" class=\"mt-0 pt-0\">\r\n                    <v-text-field\r\n                      label=\"Name*\"\r\n                      v-model=\"form.name\"\r\n                      :rules=\"rules.required\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" class=\"mt-0 pt-0\">\r\n                    <v-file-input\r\n                      truncate-length=\"15\"\r\n                      label=\"Photo\"\r\n                      v-model=\"_file\"\r\n\r\n                    ></v-file-input>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" class=\"mt-0 pt-0\">\r\n                    <v-text-field\r\n                      label=\"Short Description*\"\r\n                      v-model=\"form.shortDescription\"\r\n                      :rules=\"rules.required\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" class=\"mt-0 pt-0\">\r\n                    <v-text-field\r\n                      label=\"Long Description*\"\r\n                      v-model=\"form.longDescription\"\r\n                      :rules=\"rules.required\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" class=\"mt-0 pt-0\">\r\n                    <v-select\r\n                     v-model=\"form.type\"\r\n                      :items=\"['Static Bid', 'Blind Bid', 'Classic Bid']\"\r\n                      label=\"Type*\"\r\n                      @change=\"cascade\"\r\n                      required\r\n                    ></v-select>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" class=\"mt-0 pt-0\">\r\n                    <v-text-field\r\n                      label=\"Bid Amount\"\r\n                      v-model=\"form.bidAmount\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" class=\"mt-0 pt-0\">\r\n                    <v-text-field\r\n                      label=\"Countdown\"\r\n                      v-model=\"form.countdown\"\r\n                      v-if=\"showName\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n\r\n                  <v-col cols=\"12\" class=\"mt-0 pt-0\">\r\n                    <v-text-field\r\n                      label=\"First Warning\"\r\n                      v-model=\"form.firstWarning\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" class=\"mt-0 pt-0\">\r\n                    <v-text-field\r\n                      label=\"Second Warning\"\r\n                      v-model=\"form.secondWarning\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" class=\"mt-0 pt-0\">\r\n                    <v-datetime-picker\r\n                      label=\"Registration Date *\"\r\n                      v-model=\"form.registrationDate\"\r\n                      :text-field-props=\"textFieldProps\"\r\n                      :time-picker-props=\"timeProps\"\r\n                      time-format=\"HH:mm:ss\"\r\n                    >\r\n                      <template slot=\"dateIcon\">\r\n                        <v-icon>mdi-calendar-month</v-icon>\r\n                      </template>\r\n                      <template slot=\"timeIcon\">\r\n                        <v-icon>mdi-clock-time-nine-outline</v-icon>\r\n                      </template>\r\n                    </v-datetime-picker>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"6\" class=\"mt-0 pt-0\">\r\n                    <v-datetime-picker\r\n                      label=\"Start Date *\"\r\n                      v-model=\"form.startDate\"\r\n                      :text-field-props=\"textFieldProps\"\r\n                      :time-picker-props=\"timeProps\"\r\n                      time-format=\"HH:mm:ss\"\r\n                    >\r\n                      <template slot=\"dateIcon\">\r\n                        <v-icon>mdi-calendar-month</v-icon>\r\n                      </template>\r\n                      <template slot=\"timeIcon\">\r\n                        <v-icon>mdi-clock-time-nine-outline</v-icon>\r\n                      </template>\r\n                    </v-datetime-picker>\r\n                  </v-col>\r\n\r\n                  <v-col cols=\"12\" sm=\"6\" class=\"mt-0 pt-0\">\r\n                    <v-datetime-picker\r\n                      label=\"End Date *\"\r\n                      v-model=\"form.endDate\"\r\n                      :text-field-props=\"textFieldProps\"\r\n                      :time-picker-props=\"timeProps\"\r\n                      time-format=\"HH:mm:ss\"\r\n                      date-format=\"yyyy-MM-dd\"\r\n                    >\r\n                      <template slot=\"dateIcon\">\r\n                        <v-icon>mdi-calendar-month</v-icon>\r\n                      </template>\r\n                      <template slot=\"timeIcon\">\r\n                        <v-icon>mdi-clock-time-nine-outline</v-icon>\r\n                      </template>\r\n                    </v-datetime-picker>\r\n                  </v-col>\r\n                </v-row>\r\n              </v-container>\r\n              <small>*indicates required field</small>\r\n            </v-card-text>\r\n            <v-card-actions class=\"pb-5\">\r\n              <v-spacer></v-spacer>\r\n              <v-btn :disabled=\"loadingBtn\" @click=\"closeDialog()\">\r\n                Close\r\n              </v-btn>\r\n              <v-btn\r\n                color=\"primary\"\r\n                :loading=\"loadingBtn\"\r\n                :disabled=\"loadingBtn\"\r\n                @click=\"onSubmit()\"\r\n                class=\"px-10\"\r\n              >\r\n                Save\r\n              </v-btn>\r\n            </v-card-actions>\r\n          </v-form>\r\n        </v-card>\r\n      </v-dialog>\r\n    </v-col>\r\n  </v-row>\r\n</template>\r\n\r\n<script>\r\nimport moment from \"moment\";\r\n\r\nexport default {\r\n  name: \"createGame\",\r\n  components: {},\r\n  data() {\r\n    return {\r\n      //   this.$emit(\"obj\", true);\r\n        _file: null,\r\n      textFieldProps: {\r\n        prependIcon: \"mdi-calendar-month-outline\",\r\n        rules: [(value) => !!value || \"Required.\"],\r\n      },\r\n      timeProps: {\r\n        useSeconds: true,\r\n        ampmInTitle: true,\r\n      },\r\n\r\n      menu: false,\r\n      menu1: false,\r\n      menu2: false,\r\n      showName: true,\r\n\r\n      loadingBtn: false,\r\n      rules: {\r\n        required: [(value) => !!value || \"Required.\"],\r\n      },\r\n\r\n      startDateMenu: false,\r\n      endDateMenu: false,\r\n      dialog: false,\r\n\r\n      form: {\r\n        name: \"\",\r\n        description: \"\",\r\n        promo_startdate: \"\",\r\n        promo_enddate: \"\",\r\n        no_winners: 1,\r\n        isFilter: \"N\",\r\n        prizes: [\r\n          {\r\n            prize: \"\",\r\n            orderby: \"1\",\r\n          },\r\n        ],\r\n        filter: [],\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    cascade: function (e) {\r\n      console.log(\"Show \" + e + \" fields\");\r\n      if (e != \"Static Bid\") {\r\n        this.showName = false;\r\n      } else {\r\n        this.showName = true;\r\n      }\r\n    },\r\n\r\n    async onSubmit() {\r\n      if (!this.$refs.form.validate()) {\r\n        return false;\r\n      }\r\n\r\n      this.loadingBtn = true;\r\n      let data = this.form;\r\n      data.promo_startdate = this.formatDate(data.promo_startdate);\r\n      data.promo_enddate = this.formatDate(data.promo_enddate);\r\n      const formData = new FormData();\r\n\r\n      formData.append(\"merchantId\", this.$route.query.ID);\r\n      formData.append(\"name\", data.name);\r\n      formData.append(\"shortDescription\", data.shortDescription);\r\n      formData.append(\"longDescription\", data.longDescription);\r\n      formData.append(\"bidAmount\", data.bidAmount);\r\n      formData.append(\"registrationDate\", data.registrationDate);\r\n      formData.append(\"startDate\", data.startDate);\r\n      formData.append(\"endDate\", data.endDate);\r\n      formData.append(\"countdown\", data.countdown);\r\n      formData.append(\"firstWarning\", data.firstWarning);\r\n      formData.append(\"secondWarning\", data.secondWarning);\r\n      formData.append(\"type\", data.type);\r\n      formData.append(\"photo\", data._file);\r\n      // setTimeout(() => {\r\n      //   this.loadingBtn = false;\r\n      //   this.$refs.form.reset()\r\n      //   this.dialog = false;\r\n      //   this.$emit(\"obj\", true);\r\n      // }, 3000);\r\n\r\n      await this.$api\r\n        .post(\"events\", formData)\r\n        .then((response) => {\r\n          console.log(response);\r\n        //  this.$emit(\"obj\", true);\r\n         // this.loadingBtn = false;\r\n          this.$refs.form.reset();\r\n          this.dialog = false;\r\n          alert(\"Raffle is successfully created\");\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n          this.loadingBtn = false;\r\n        });\r\n\r\n      // this.$store.dispatch(\"addOverlay\", true);\r\n    },\r\n\r\n    closeDialog() {\r\n      this.dialog = false;\r\n      this.$refs.form.reset();\r\n    },\r\n\r\n    formatDate(d) {\r\n      return moment(d).format(\"YYYY-MM-DD HH:mm:ss\");\r\n    },\r\n  },\r\n  computed: {\r\n    getPartner() {\r\n      return this.$store.getters.getPartner;\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":"src/views/dialogs"}]}